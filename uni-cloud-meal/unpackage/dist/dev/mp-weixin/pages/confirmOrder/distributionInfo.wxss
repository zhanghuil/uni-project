page {
  background: #FAFAFA;
}
.disInfo-form-group {
  padding: 0 28rpx;
  background: #FFFFFF;
}
.disInfo-form-group .item {
  padding: 32rpx 0;
  border-bottom: 1rpx solid #EEEFF3;
}
.disInfo-form-group .item:last-child {
  border: none;
}
.disInfo-form-group .item .star {
  font-size: 40rpx;
  color: #FF4D23;
}
.disInfo-form-group .item .title {
  font-size: 30rpx;
  color: #2A2A2A;
  padding-bottom: 24rpx;
}
.disInfo-form-group .item .ipt {
  font-size: 28rpx;
  color: #6A6A6A;
}
.disInfo-form-group .item .arrow {
  position: relative;
  padding-right: 40rpx;
}
.disInfo-form-group .item .arrow:before {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  display: block;
  margin: auto;
  width: 15px;
  height: 15px;
  color: #6A6A6A;
  content: "\e6a3";
  text-align: center;
  font-size: 17px;
  font-family: cuIcon;
  line-height: 15px;
}
.disInfo-form-group .item .defaultTxt {
  font-size: 28rpx;
  color: #C0C0C0;
}
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.footer .cu-btn {
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  width: 100%;
  height: 90rpx;
  -webkit-border-radius: 0;
          border-radius: 0;
}
.footer .cu-btn.bg-grey {
  background: #C0C0C0;
}
.footer .cu-btn.bg-cyan {
  background-color: #4EC09B;
}
.cu-modal.bottom-modal .cu-dialog {
  background-color: #FFFFFF;
  -webkit-border-radius: 24rpx 24rpx 0 0;
          border-radius: 24rpx 24rpx 0 0;
}
.selectAddress .cuIcon-close {
  position: absolute;
  right: 40rpx;
  top: 44rpx;
  color: #C0C0C0;
  font-size: 40rpx;
  width: 36rpx;
  margin-left: 42rpx;
}
.selectAddress .topBox {
  padding: 44rpx 40rpx 60rpx 28rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 28rpx;
  color: #4EC09B;
  height: 150rpx;
}
.selectAddress .topBox .icon {
  color: #4A4A4A;
  margin-right: 16rpx;
  font-size: 40rpx;
}
.selectAddress .topBox .px10 {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.selectAddress .content {
  padding: 0 28rpx 32rpx;
}
.selectAddress .content .scroll-Y {
  height: 520rpx;
  position: relative;
}
.selectAddress .content .pb30 {
  padding-bottom: 60rpx;
}
.selectAddress .content .item {
  text-align: left;
  font-size: 28rpx;
  padding-bottom: 32rpx;
  color: #4A4A4A;
}
.selectAddress .content .item.on {
  color: #4EC09B;
}

