.tit {
  font-size: 36rpx;
  color: #4EC09B;
  padding: 48rpx 0 0;
  text-align: center;
}
.chooseBox {
  text-align: center;
}
.chooseBox .item {
  position: relative;
}
.chooseBox .item .img {
  width: 200rpx;
  height: 200rpx;
  margin-top: 80rpx;
}
.chooseBox .item .radioSex {
  font-size: 40rpx;
  color: #4EC09B;
  position: absolute;
  bottom: 6rpx;
  right: 10rpx;
}
.chooseBox .item .name {
  font-size: 30rpx;
  color: #2A2A2A;
  padding-top: 48rpx;
}
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.footer .cu-btn {
  border: none;
  background: #4EC09B;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;
  width: 100%;
  height: 90rpx;
  -webkit-border-radius: 0;
          border-radius: 0;
}

