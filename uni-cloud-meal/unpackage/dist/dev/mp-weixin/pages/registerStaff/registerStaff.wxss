.staff-form-group {
  padding: 0 28rpx;
}
.staff-form-group .item {
  padding: 32rpx 0;
  border-bottom: 1rpx solid #EEEFF3;
}
.staff-form-group .item .title {
  font-size: 30rpx;
  color: #2A2A2A;
  padding-bottom: 24rpx;
}
.staff-form-group .item .ipt {
  font-size: 28rpx;
  color: #6A6A6A;
}
.staff-form-group .item .f12 {
  font-size: 24rpx;
}
.staff-form-group .item .mt7 {
  margin-top: 14rpx;
}
.staff-form-group .item .tipImg {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}
.staff-form-group .item .uni-input {
  font-size: 28rpx;
  color: #6A6A6A;
}
.staff-form-group .item .codeTxt {
  font-size: 26rpx;
  position: relative;
  padding-left: 38rpx;
}
.staff-form-group .item .codeTxt::before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 2rpx;
  height: 32rpx;
  background: #F4F4F4;
}
.submitBtn {
  margin-top: 160rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  height: 80rpx;
  padding: 0 128rpx;
}
.submitBtn.bg-gray {
  background: #C0C0C0;
}
.toast {
  background: #FF946B;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  font-size: 12px;
  color: #FFFFFF;
  padding: 16rpx 32rpx;
  position: absolute;
  top: 82rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.toast.success {
  background: #4EC09B;
}
.cu-dialog {
  -webkit-border-radius: 24rpx;
          border-radius: 24rpx;
  background: #FFFFFF;
}
.closeBtn {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  font-size: 40rpx;
}
.failImg {
  width: 134rpx;
  height: 134rpx;
  margin: 48rpx 0 16rpx;
}
.failTxt {
  line-height: 42rpx;
  font-size: 28rpx;
  color: #4A4A4A;
  padding: 48rpx 32rpx 64rpx;
}
.failBtnBox {
  padding: 0 28rpx 48rpx;
}
.failBtnBox .cu-btn {
  padding: 0 76rpx;
  height: 80rpx;
}
.failBtnBox .cu-btn.btn1 {
  padding: 0 0 0 20rpx;
  background: none;
  color: #C0C0C0;
}

